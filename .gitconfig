[user]
        name = Evan.Weible
        email = ekweible@gmail.com
[core]
        excludesfile = /Users/evanweible/.gitignore_global
[color]
        ui = true
[diff]
        tool = p4mergetool
[difftool "p4mergetool"]
        cmd = /Applications/p4merge.app/Contents/Resources/launchp4merge $LOCAL $REMOTE
[merge]
        tool = p4mergetool
[mergetool "p4mergetool"]
        cmd = /Applications/p4merge.app/Contents/Resources/launchp4merge $PWD/$BASE $PWD/$REMOTE $PWD/$LOCAL $PWD/$MERGED
        trustExitCode = false
[mergetool]
        keepBackup = true
[alias]
        co = checkout
        st = status
        br = branch

